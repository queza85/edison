	<portlet>
		<portlet-name>edison-bestsimulation</portlet-name>
		<display-name>Edison Simulation</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>classpath:org/kisti/edison/bestsimulation/portlet/simulation/edison-bestsimulation-portlet.xml</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/WEB-INF/html/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Edison Simulation</title>
			<short-title>Edison Simulation</short-title>
			<keywords>Edison Simulation</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>


/*-- SW 기본 내용 테이블 스타일 ---*/

table.swview {
    width:100%;
    border-collapse:collapse;
}
table.swview td {
    border-bottom: 1px solid #D5D5D5;
	vertical-align:middle;
}


/*-------- 탭 -------*/
#tabs_container {
	width: 1149px;
	margin: 0 auto; 
	height: 34px;
	padding: 19px 10px 10px 10px; 
	background-image:url(/edison-portlet/images/btn/sw_view_tap_bg.jpg)
}

#tabs{
	list-style: none;
	margin: 0 0 0 10px;
	font: 12px malgun gothic;
}

#tabs li {
	list-style:none;
	display: inline;
}
#tabs li a {
	border: 2px solid #d2d2d2;
	padding: 8px 50px;
	color : #646464;
	text-decoration: none;
	background-color:  #eef8fa;
	border-bottom: none;
	outline: none;
	border-radius: 5px 5px 0 0;
	-moz-border-radius: 5px 5px 0 0;
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 5px;
}

#tabs li a:hover, #tabs li a.active{
	font: 13px malgun gothic;
	color : #FFFFFF;
	font-weight : bolder;
	padding-top:13px;
	background-color:#7ABCED;
	border: 3px solid #d2d2d2;
	border-bottom: none;
}

.tab_content {
	display: none;
}


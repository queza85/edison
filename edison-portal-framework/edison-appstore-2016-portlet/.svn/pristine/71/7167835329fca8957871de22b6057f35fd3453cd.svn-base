/*---- 파라미터 정보 TABLE ------------------*/
#pra_wrap {
	width:1115px; 
	margin:0 auto; 
	padding:0px 15px 5px 15px;
	font-size: 12px;
}

table.pralist {
	width:100%;
	border-collapse:collapse;
	border-bottom: 1px solid #D5D5D5;
}

table.pralist th {
	color:#434343;
	text-align:center;
	line-height:40px;
	background-color: #eef9ff;
	height:40px;
	border-top: 1px solid #D5D5D5;
}
table.pralist td {
    color:#595959;
    text-align:center;
    border-top: 1px solid #D5D5D5;
    height:60px;
	vertical-align:middle;
}

/* kd 추가 */
table.pralist td.TL{
	text-align:left;
	padding-left: 10px;
}

/*---- 문제필터  ------*/

#filter_wrap{
	padding:15px; 
	width: 1139px; 
	margin: 0 auto; 
}
table.filter {
	width:100%;
	border-collapse:collapse;
	border-top: 1px solid #D5D5D5;
	border-bottom: 1px solid #D5D5D5;
	border-left: 1px solid #D5D5D5;
	border-right: 1px solid #D5D5D5;
	font-size:12px;
	color:#444444;
}
table.filter th {
	color:#434343;
	text-align:center;
	background-color: #eef9ff;
	height:32px;
	border-left: 1px solid #D5D5D5;
	border-right: 1px solid #D5D5D5;

}
table.filter td {
	color:#595959;
	padding: 0px 0px 0px 7px;
	border-top: 1px solid #D5D5D5;
	height:32px;
	border-left: 1px solid #D5D5D5;
	border-right: 1px solid #D5D5D5;
}

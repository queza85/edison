	<portlet>
		<portlet-name>edison-content</portlet-name>
		<display-name>Edison Content</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>classpath:org/kisti/edison/content/portlet/content/edison-content-portlet.xml</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/WEB-INF/html/content/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Edison Content</title>
			<short-title>Edison Content</short-title>
			<keywords>Edison Content</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	

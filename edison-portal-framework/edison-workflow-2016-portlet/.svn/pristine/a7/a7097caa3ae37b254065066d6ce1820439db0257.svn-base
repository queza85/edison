.wftitlebox001 > h2{color: #fff !important; font-size: 18px !important; font-weight: 600 !important; padding-top: 5px !important;}

.wf-container{width: 100%; height: 750px;}
.wf-container .lefttabm{position: absolute; left: 10px;}
.lefttreemenu1.wf-jstree {padding-top:45px;}
.lefttreemenu1.wf-jstree > ul{height: 700px; overflow-y: auto;}
.lefttreemenu1.wf-jstree ul{margin:0; padding:0;}
.lefttreemenu1.wf-jstree ul li{list-style:none; font-size:15px; color:#666; background:url(../images/jstree/myfile/myfile-icon02.png)no-repeat 0 3px; line-height:30px;}
.lefttreemenu1.wf-jstree ul li.jstree-open{background:url(../images/jstree/myfile/myfile-icon01.png)no-repeat 0 3px;}
.lefttreemenu1.wf-jstree ul li.jstree-close{background:url(../images/jstree/myfile/myfile-icon02.png)no-repeat 0 3px;}
.lefttreemenu1.wf-jstree ul li a:link{color:#666;}
.lefttreemenu1.wf-jstree ul li  a:hover{text-decoration:underline;}
.lefttreemenu1.wf-jstree ul li ul{margin:0; padding:0;}
.lefttreemenu1.wf-jstree ul li ul li{font-size:15px; font-weight:normal; list-style:none; background:url(../images/jstree/myfile/myfile-icon03.png) no-repeat 0 3px; line-height:30px;}
.lefttreemenu1.wf-jstree ul li ul li.jstree-open{background:url(../images/jstree/myfile/myfile-icon04.png)no-repeat 0 3px;}
.lefttreemenu1.wf-jstree ul li ul li.jstree-close{background:url(../images/jstree/myfile/myfile-icon03.png)no-repeat 0 3px;}
.lefttreemenu1.wf-jstree ul li ul li a:link{color:000;}
.lefttreemenu1.wf-jstree ul li ul li a:hover{text-decoration:underline;}
.lefttreemenu1.wf-jstree ul li ul li ul li{background:url(../images/Workflow/file_icon.png)no-repeat 0 3px;}

.leftwrap{height: 100%;}
.rightwrap{height:100%;}
.wfconwrap{height:100%; margin-bottom: 30px; float: left;}
.lefttreemenu{height: 99%;}
.leftlistbox{height: 99%;}
.lefttablemenu{height: 99%; clear: both;}

.wf-container > .alert{ width: 786px; left: 380px; position: absolute; display: none;}

.apparea{position: relative; padding:0px; min-height:670px; height: 100%;}
.wf-box{padding: 30px 5px 5px 20px; width: 125px !important; height:130px !important; position: absolute; cursor: move;}
.lefttabm ul li.on {font-size: 16px;}
.lefttabm > ul > li{cursor: pointer;}
.endpointDrag{cursor: pointer;}
.input-port.dragActive { border:2px dotted orange; }
/* .dropHover { border:1px dotted red; } */

.openslidearrow{width: 32px; height: 51px;}

.jstree *,.jsplumb-drag-select *{
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;    
}

.wf-box, .jstree-leaf{white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}

.hidden{display: none;}
.jsplumb-endpoint:hover{cursor: pointer; z-index: 9999;}

.wftitlebox001 > input[type="text"]{width: 64%; margin-bottom: 2px;}
.wftitlebox001 > span > input[type="button"]{margin-right: 5px;}
.wftitlebox001 > h2 {float: left;}

.lefttablemenu{width:98%; border:solid 1px #00c7e5; min-height:589px; clear:both; position:relative; top:-5px; padding:15px 10px 10px 10px;}
.lefttablemenu > .searchbox {float:right !important; margin-top:-5px; margin-right:20px;}
.slideclosearrow{position: absolute; right:0; top: -1px; z-index:100;}

.tablemf_list table{ width: 100%; border-collapse:collapse;} 
.tablemf_list table td{border:none; outline:none;} 

.ui-selectable-helper { position: absolute; z-index: 100; border:1px dotted black; }
.ui-selected{border: solid 1px #555555; box-shadow: 3px 3px 7px #c6c6c6;}
.ui-selecting{background-color: rgba( 30, 30, 30,0.4 ); box-shadow: 3px 3px 7px #c6c6c6; border: solid 1px #000;}

.wrap{margin-top: 40px;}

.date > span{margin-right: 10px;}

/* workflow list */
.tablemf_list tr.on{ background-color: #D2D1D1;}
.titleTd{cursor: pointer;}

.listbox{cursor: pointer; width:300px;}
.listbox.on{width:300px; margin:0 auto; padding:7px 15px; background-color:#8db9e5; border-radius:3px; margin-bottom:12px; border:solid 1px #8db9e5;}
.listbox.on ul{margin:0; padding:0;}
.listbox.on ul li{list-style:none; font-size:15px; font-weight:600; color:#fff; line-height:2.0em;}
.listbox.on ul li a{color:#fff !important; }
.listbox.on ul li a:link{color:#fff !important; }
.listbox.on ul li a:hover{text-decoration:underline;}
.listbox.on ul li.date{list-style:none; font-size:15px; font-weight:500; color:#000; line-height:1.5em;}
.listbox.on ul li.date span{color:#f4fd02;}
.listbox.navigation:hover{background-color:#8db9e5;}
.listbox.navigation ul li{text-align: center;}
.listbox.navigation ul li a{text-decoration: none;}
.listbox.navigation ul li a:hover{color:#fff !important;}

.workflow-list-div{overflow-y: auto; height: 700px;}

/* form validation*/
.form-validation-error{border-color: #b50303 !important; -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075) !important; -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075) !important; box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075) !important;}
.form-validation-error::-webkit-input-placeholder { /* Chrome */ color: #b50303 !important;}
.form-validation-error:-ms-input-placeholder { /* IE 10+ */ color: #b50303 !important;}
.form-validation-error::-moz-placeholder { /* Firefox 19+ */ color: #b50303 !important;}
.form-validation-error:-moz-placeholder { /* Firefox 4 - 18 */ color: #b50303 !important;}